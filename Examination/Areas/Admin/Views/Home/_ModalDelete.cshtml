@model Examination.Data.Question

<form asp-action="DeleteQuestions">
    @await Html.PartialAsync("_ModalHeader", new ModalHeader
{
    Heading = "Test savolini o'chirish"
})

    <div class="modal-body">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="ID" />
        <input type="hidden" asp-for="TestID" />

       
        <div class="form-group">
            <label asp-for="Nomi" class="control-label"></label>
            <input asp-for="Nomi" class="form-control "   />
            <span asp-validation-for="Nomi" class="text-danger"></span>
        </div>

    </div>




    @await Html.PartialAsync("_ModalFooter", new ModalFooter {SubmitButtonText = "O'chirish",classButton="btn btn-danger" });
</form>
@await Html.PartialAsync("_ValidationScriptsPartial")